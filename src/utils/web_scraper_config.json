{
  "websites": {
    "example_site": {
      "url": "https://example.com",
      "description": "Example website for demonstration",
      "added_date": "2025-10-09T00:00:00"
    },
    "search_engine": {
      "url": "https://www.google.com",
      "description": "Google search for data extraction",
      "added_date": "2025-10-09T00:00:00"
    }
  },
  "scraping_rules": {
    "extract_title": {
      "website": "example_site",
      "selector": "h1",
      "action_type": "extract",
      "parameters": {
        "selector_type": "css"
      }
    },
    "fill_search_box": {
      "website": "search_engine",
      "selector": "input[name='q']",
      "action_type": "fill",
      "parameters": {
        "value": "sample search term",
        "selector_type": "css"
      }
    },
    "click_search_button": {
      "website": "search_engine",
      "selector": "input[name='btnK']",
      "action_type": "click",
      "parameters": {
        "selector_type": "css"
      }
    },
    "extract_search_results": {
      "website": "search_engine",
      "selector": "#search .g h3",
      "action_type": "extract",
      "parameters": {
        "selector_type": "css"
      }
    },
    "wait_for_results": {
      "website": "search_engine",
      "selector": "#search",
      "action_type": "wait",
      "parameters": {
        "timeout": 10,
        "selector_type": "css"
      }
    }
  },
  "excel_mapping": {
    "title_data": "web_title",
    "search_result": "web_search_result",
    "timestamp": "web_timestamp"
  }
}
